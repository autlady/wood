                  .header-col
                     a(href="#!" target="_blank" rel="noopener noreferrer").contact-link
                        img(src="./img/icons/telephone.webp", alt="icon", width="20")
                        span +7 (3452) 60-66-98
                     a(href="#!" target="_blank" rel="noopener noreferrer").contact-link
                        img(src="./img/icons/mobile.webp", alt="icon", width="20")
                        span +7 (929) 269-66-98
               .header-btns
                  .social-block
                     a(href="#!" target="_blank" rel="noopener noreferrer").social-link
                        img(src="./img/icons/viber.webp", alt="icon", width="45" height="45")
                     a(href="#!" target="_blank" rel="noopener noreferrer").social-link
                        img(src="./img/icons/wa.webp", alt="icon", width="45" height="45")
                     a(href="#!" target="_blank" rel="noopener noreferrer").social-link
                        img(src="./img/icons/telegram.webp", alt="icon", width="45" height="45")
                  button.btn Заказать звонок
