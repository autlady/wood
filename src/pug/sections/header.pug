header.header
   // HEADER TOP ROW
   .header-top
      .container
         .header-row
            a(href="#!").header-logo
               img(src="./img/logo.webp" srcset="./img/logo@2x.webp" alt="logo" width="67" height="90" loading="lasy")
            .header-wrapper
               .header-contacts
                  .header-col
                     a(href="#!" target="_blank" rel="noopener noreferrer").contact-link
                        img(src="./img/icons/clock.webp", alt="icon", width="20")
                        span Ежедневно 9.00-21.00
                     a(href="#!" target="_blank" rel="noopener noreferrer").contact-link
                        img(src="./img/icons/partners.webp", alt="contact", width="20")
